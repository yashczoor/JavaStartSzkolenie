<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox stylesheets="@application.css"
	xmlns="http://javafx.com/javafx/10.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.TextPaneController">
	<children>
		<TextArea fx:id="textArea" maxHeight="1.7976931348623157E308"
			maxWidth="1.7976931348623157E308" prefHeight="200.0"
			prefWidth="200.0" VBox.vgrow="ALWAYS" />
		<Button fx:id="button" maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" text="" VBox.vgrow="ALWAYS" />
	</children>
</VBox>
